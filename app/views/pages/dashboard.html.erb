<div class="container">
  <div class="card-container">
    <% @bookings.each do |booking| %>
    <div class="card" style="width: 18rem;">
      <img class="card-img-top" src="https://source.unsplash.com/random" alt="Card image cap">
      <div class="card-body">
        <h5 class="card-title"><%= booking.item.title %></h5>
        <p class="card-text"><%= booking.item.description %></p>
        <p class="card-text text-right"><%= booking.item.price %> $</p>
        <p class="card-text text-left">total: $</p>
        <p class="card-text text-left">date: <%= booking.end_date %> $</p>
        <p class="card-text text-left">date: <%= booking.start_date %> $</p>


      </div>
    </div>
      <% end %>
      </div>
    </div>
